book-item/book-item.js",["291"],"import React from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport { BookAuthor } from \"../book-author\";\nimport { BookBuy } from \"../book-buy\";\nimport { BookCover } from \"../book-cover/book-cover\";\nimport { setCurrentBook } from \"../../store/books/reducer\";\nimport \"./book-item.css\"; \n\nexport const BookItem = ({ book:book }) => {\n  const history = useHistory();\n  const dispatch = useDispatch();\n  const handleClick = () => {\n    dispatch(setCurrentBook(book));\n    history.push(`/app/${book.title}`);\n  };\n\n  return (\n    <div className=\"book-item\" onClick={handleClick}>\n      <BookCover image={book.image} />\n      <div className=\"book-item__details\">\n        <span className=\"book-item__title\">{book.title}</span>\n        <div className=\"book-item__author\">\n          {book.authors.map((author) => (\n            <BookAuthor author={author} key={author} />\n          ))}\n        </div>\n        <div className=\"book-item__buy\">\n          <BookBuy book={book} />\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/turgunbekrustamov/Desktop/book-store/src/components/book-cover/book-cover.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/components/book-buy/index.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/components/book-author/index.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/components/book-author/book-author.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/components/book-buy/book-buy.js",["292"],"import React from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Button } from \"../button\";\nimport { setItemInCart, deleteItemFromCart } from \"../../store/cart/reducer\";\nimport \"./book-buy.css\"; \n\nexport const BookBuy = ({ book:book }) => {\n  const dispatch = useDispatch();\n  const items = useSelector((state) => state.cart.itemsInCart);\n  const isItemInCart = items.some((item) => item.id === book.id);\n\n  const handleClick = (e) => {\n    e.stopPropagation();\n    if (isItemInCart) {\n      dispatch(deleteItemFromCart(book.id));\n    } else {\n      dispatch(setItemInCart(book));\n    }\n  };\n\n  return (\n    <div className=\"book-buy\">\n      <span className=\"book-buy__price\">{book.price} сом.</span>\n      <Button\n        type={isItemInCart ? \"secondary\" : \"primary\" }\n        onClick={handleClick}\n      >\n        {isItemInCart ? \"Добавлено в избранное\" : \"В избранное\"}\n      </Button>\n    </div>\n  );\n};\n","/Users/turgunbekrustamov/Desktop/book-store/src/store/books/reducer.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/pages/book-page/index.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/pages/book-page/book-page.js",[],"/Users/turgunbekrustamov/Desktop/book-store/src/components/sort-products/sort-products.jsx",["293","294","295","296"],"import { useSelector } from \"react-redux\";\nimport { Button } from \"../button\";\nimport { CartItem } from \"../cart-item\";\nimport {BOOKS} from \"../../pages/home-page/home-page\"\nimport { BookItem } from '../../components/book-item';\n\n\nimport React, { useState, useEffect } from 'react';\nconst movies = [\n  {\n    id: 1,\n    name: 'Matrix',\n    country: 9,\n    collection: 300, //in CRs\n    releasedOn: 1999,\n  },\n  {\n    id: 2,\n    name: 'Tere Nam',\n    country: 3,\n    collection: 101,\n    releasedOn: 2004,\n  },\n  {\n    id: 3,\n    name: 'Bahubali',\n    country: 4,\n    collection: 500,\n    releasedOn: 1987,\n  },\n];\nfunction SortProduct() {\n\n  const [data, setData] = useState([]);\n  const [sortType, setSortType] = useState('albums');\n\n\n  useEffect(() => {\n    const sortArray = type => {\n      const types = {\n        title: 'title',\n        authors: 'authors',\n      };\n      const sortProperty = types[type];\n      const sorted = [...BOOKS].sort((a, b) => b[sortProperty] - a[sortProperty]);\n      setData(sorted);\n    };\n    sortArray(sortType);\n  }, [sortType]);\n  \n  \n  return (\n    <div className=\"App\">\n      <select onChange={(e) => setSortType(e.target.value)}> \n        <option value=\"title\">Title</option>\n        <option value=\"authors\">Authors</option>\n      </select>\n\n      <div className=\"home-page\">\n            { data.map(book => <BookItem book={book} key={book.id}/>) }\n\n        </div>\n      {/* {data.map(book => (\n        <div key={book.id} style={{ margin: '30px' }}>\n          <div>{`Title: ${book.title}`}</div>\n          <div>{`Authors: ${book.authors}`}</div>\n        </div>\n      ))} */}\n    </div>\n  );\n}\nexport default SortProduct;\n\n//   const items = useSelector((state) => state.cart.itemsInCart);\n//   return (\n//     <div className=\"cart-menu\">\n//       <div className=\"cart-menu__books-list\">\n//         {items.length > 0\n//           ? items.map((book) => (\n//               <CartItem\n//                 key={book.image}\n//                 image={book.image}\n//                 price={book.price}\n//                 title={book.title}\n//                 id={book.id}\n//               />\n//             ))\n//           : \"Список желаемых книг пуст\"}\n//       </div>\n//       {items.length > 0 ? (\n//         <div className=\"cart-menu__arrange\">\n//           <div className=\"cart-menu__total-price\">\n//             <span>Итого:</span>\n//             <span>{calcTotalPrice(items)} сом.</span>\n//           </div>\n//           <Button type=\"primary\" size=\"m\" onClick={onClick}>\n//             Оформить заказ\n//           </Button>\n//         </div>\n//       ) : null}\n//     </div>\n//   );\n// };\n\n\n ",["297","298"],"/Users/turgunbekrustamov/Desktop/codify-lab/src/index.js",[],"/Users/turgunbekrustamov/Desktop/codify-lab/src/reportWebVitals.js",[],"/Users/turgunbekrustamov/Desktop/codify-lab/src/App.js",[],"/Users/turgunbekrustamov/Desktop/codify-lab/src/store/index.js",[],"/Users/tu